{
    "manifest_version": 3,
    "name": "amongus ventr",
    "version": "1.0",
    "description": "vencord but for lyntr.",
    "permissions": [
      "activeTab",
      "storage"
    ],
      "content_scripts": [
        {
          "matches": ["https://lyntr.com/*"],
          "js": ["content.js"]
        }
      ],
      "action": {
        "default_popup": "popup.html",
        "default_icon": {
          "16": "icons/icon16.png",
          "48": "icons/icon48.png",
          "128": "icons/icon128.png"
        }
      }
    }